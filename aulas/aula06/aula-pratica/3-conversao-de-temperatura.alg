algoritmo "Creuza 03"
// Função : Creuza não sabe a temperatura.
// Autor : Soryu Yamato.
// Data : 9/11/2020
var

   fahrenheit,celsius:Real
   
inicio

      Escreva("Qual é a temperatura aqui? ")
      Leia(fahrenheit)
      celsius <- (fahrenheit - 32) / 1.8
      Escreval("No Brasil estaria ",celsius:2:1,"C°")

fimalgoritmo
