algoritmo "Contar de 0 até 10"
// Função : O contador vai de 0 até 10.
// Autor : Soryu Yamato.
// Data : 9/12/2020
var

   contador:Inteiro
   
inicio

      contador <- 0
      Enquanto (contador<=10) faca
          Escreval(contador)
          contador <- contador + 1
      FimEnquanto
      Escreval("Terminei de contar")
      
fimalgoritmo
